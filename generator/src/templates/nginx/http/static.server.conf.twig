{% extends "nginx/http/server.conf.twig" %}
{% block locations %}

    location / {
        index index.html
        access_log  off;
        expires     7d;
        add_header  Pragma public;
        add_header  Cache-Control "public, must-revalidate, proxy-revalidate";
        etag on;
        try_files   $uri $uri/ =404;
    }

    root /data/public/{{ endpointData['entry-point'] }};
{% endblock locations %}
